    <select id="selectTodayCourseByBeginTime" parameterType="java.util.Map" resultType="org.linlinjava.litemall.db.vo.LitemallTeamCourseVO">
        SELECT
            lc.id courseId,
            lc.begin_time beginTime,
            lc.end_time endTime,
            lc.course_name courseName,
            lc.course_type courseType,
            lt.trainer_name trainerName,
            lc.max_count maxCount,
            lc.course_price coursePrice
        FROM
            litemall_course lc
            LEFT JOIN litemall_trainer lt ON LC.trainer_id = LT.id
        WHERE lc.course_type = '0'
        AND date_format(begin_time,'%Y-%m-%d') = #{date,jdbcType=VARCHAR}
        AND lc.deleted = 0
        ORDER BY lc.sort_order
    </select>
